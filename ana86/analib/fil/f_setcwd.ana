module H
	c_name "fil","_",1
	import fil

*proc setcdir(dir:d);cdecl
enter
	save dx,ds
	ds.dx = dir
	ah = 0x3b
	intr 0x21
	if (cf == 0)
		ax = 0
	fi
	neg ax
	return
endproc

*proc setcwd(dir:d);cdecl
enter
	save dx,si,ds
	
	ds.si = dir
	dx = 0
	ax = w[si]
	if (ah == ':')
		if (al >= 'a' && al <= 'z')
			al -= 0x20
		fi
		al -= 'A'
		dl = al
		++dl
	fi
	setcdir ds.si
	if (ax == 0 && dx)
		--dl
		ah = 0x0e
		intr 0x21
		ax = 0
	fi
	return
endproc

endmodule
