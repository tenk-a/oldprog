
# --------------------------------------------------------------------------- #

.ana.obj:
	ANA -q1 -c -o$(TMP) -1 -dFC= $<
	asm /ML $(TMP)$*,$*,NUL,NUL

.as.obj:
	asm /ML $*.as,$*,NUL,NUL

$(LIBNAME).lib : $(OBJ)
	tlib $(LIBNAME) /c /e @myl.lnk

# --------------------------------------------------------------------------- #

